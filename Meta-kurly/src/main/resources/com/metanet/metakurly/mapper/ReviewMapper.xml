<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.metanet.metakurly.mapper.ReviewMapper">

	<select id="getList"
		resultType="com.metanet.metakurly.domain.ReviewDTO">

	<![CDATA[
  		select * from review where r_id > 1
  	]]>

	</select>

	<insert id="insert">
		insert into review values(review_seq.nextval, #{m_id}, sysdate, #{contents},
		#{p_id})
	</insert>


</mapper>